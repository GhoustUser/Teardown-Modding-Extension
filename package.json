{
	"name": "teardown-lua-api",
	"displayName": "Teardown lua API",
	"description": "Adds Teardown's Lua API to the Lua language server for better autocompletion and documentation support.",
	"author": {
		"name": "GhoustUser",
		"url": "https://gitlab.com/GhoustUser"
	},
	"version": "0.1.0",
	"main": "./extension.js",
	"keywords": [
		"teardown",
		"modding",
		"lua",
		"api"
	],
	"icon": "./icon.png",
	"engines": {
		"vscode": "^1.85.0"
	},
	"activationEvents": [
		"workspaceContains:info.txt"
	],
	"contributes": {
		"customEditors": [
			{
				"viewType": "CustomEditor-info_txt",
				"displayName": "Custom Info Editor",
				"selector": [
					{
						"filenamePattern": "info.txt"
					}
				]
			}
		]
	}
}